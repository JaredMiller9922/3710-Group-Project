module keyboard_with_leds(
    input wire clk,
    input wire ps2_clk,
    input wire ps2_data,
    output wire [7:0] leds
);

    ps2_keyboard keyboard (
        .clk(clk),
        .ps2_clk(ps2_clk),
        .ps2_data(ps2_data),
        .scan_code(scan_code),
        .new_data(new_data)
    );

    assign leds = scan_code; // Display scan code on LEDs
endmodule
